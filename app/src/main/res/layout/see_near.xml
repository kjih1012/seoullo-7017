<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <ImageView
        android:id="@+id/map"
        android:adjustViewBounds="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/menu" />

    <Button
        android:id="@+id/jonlimdongButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/manlidongButton"
        android:layout_alignEnd="@+id/cheongpadongButton"
        android:layout_marginEnd="70dp"
        android:background="#00000000" />

    <Button
        android:id="@+id/manlidongButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/jonlimdongButton"
        android:layout_centerVertical="true"
        android:layout_marginStart="14dp"
        android:background="#00000000" />

    <Button
        android:id="@+id/namdeamunbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/huihyeondongButton"
        android:layout_toEndOf="@+id/manlidongButton"
        android:background="#00000000" />

    <Button
        android:id="@+id/huihyeondongButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/cheongpadongButton"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="26dp"
        android:layout_toEndOf="@+id/manlidongButton"
        android:background="#00000000" />

    <Button
        android:id="@+id/cheongpadongButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/manlidongButton"
        android:layout_marginStart="178dp"
        android:background="#00000000" />


</RelativeLayout>
